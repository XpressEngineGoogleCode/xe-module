<query id="getLastloggedIpaddress" action="select">
    <tables>
        <table name="loginlog" />
    </tables>
    <columns>
        <column name="ipaddress" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" filter="number" />
    </conditions>
    <navigation>
        <list_count var="list_count" default="1" />
    </navigation>
</query>