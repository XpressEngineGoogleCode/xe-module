<filter name="revert" module="pointsend" act="procPointsendAdminRevert">
	<form>
		<node target="log_srl" required="true" filter="number" />
	</form>
	<parameter>
		<param name="log_srl" target="log_srl" />
	</parameter>
	<response callback_func="completeRevert">
		<tag name="error" />
		<tag name="message" />
	</response>
</filter>