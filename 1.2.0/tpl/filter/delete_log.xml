<filter name="delete_log" module="pointsend" act="procPointsendAdminDeleteLog">
	<form>
		<node target="log_srl" required="true" filter="number" />
	</form>
	<parameter>
		<param name="log_srl" target="log_srl" />
	</parameter>
  <response callback_func="completeDeleteLog">
	  <tag name="error" />
	  <tag name="message" />
  </response>
</filter>