<filter name="rollback" module="pointsend" act="procPointsendAdminRollback">
	<form>
		<node target="log_srl" required="true" filter="number" />
	</form>
	<parameter>
		<param name="log_srl" target="log_srl" />
  </parameter>
  <response callback_func="completeRollback">
      <tag name="error" />
      <tag name="message" />
  </response>
</filter>
